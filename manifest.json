{
  "manifest_version": 3,
  "name": "Website Productivity Tracker",
  "version": "1.0",
  "description": "Track and manage time spent on websites to boost productivity",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "alarms",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
